{
  "Class": "ecs_service",
  "DockerImage": "${DOCKER_IMAGE}",
  "Environment": "${ENVIRONMENT}",
  "Project": "${PROJECT}",
  "AppName": "${APP_NAME}",
  "Owner": "${OWNER}",
  "AccountPrefix": "${AWS_ACCOUNT_PREFIX}",
  "Route53ZoneName": "sandbox.internal",
  "WithBackgroundTask": "true",
  "WithBackgroundTaskHealthCheck": "true",
  "Scheme": "int",
  "Priority": "1",
  "ApplicationVariables": [
    {
      "Name": "PORT",
      "Value": "8080"
    }
  ]
}
